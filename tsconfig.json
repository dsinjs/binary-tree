{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "ES2018",
      "ES2019",
      "es2017",
      "es7",
      "es6",
      "es5",
      "dom"
    ],
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
